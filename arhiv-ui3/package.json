{
  "name": "arhiv-ui3",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "description": "arhiv UI",
  "main": "",
  "scripts": {
    "lint": "eslint .",
    "build:js": "node build.js",
    "build:css": "TAILWIND_MODE=build postcss ./src/index.css -o public/index.css",
    "watch:js": "node build.js --watch",
    "watch:css": "TAILWIND_MODE=watch postcss --watch ./src/index.css -o public/index.css",
    "prod:build:js": "NODE_ENV=production yarn build:js",
    "prod:build:css": "NODE_ENV=production yarn build:css",
    "prod:build": "yarn run prod:build:js && yarn run prod:build:css"
  },
  "dependencies": {
    "a11y-dialog": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.2.6",
    "esbuild": "^0.14.5",
    "eslint": "^8.1.0",
    "postcss": "^8.3.0",
    "postcss-cli": "^9.0.1",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.3.5"
  }
}
