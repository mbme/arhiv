<div class="modal">
  <div class="modal-title">
    Scraped documents
  </div>

  <div class="modal-content">
    <div class="mb-4">
      Source: {{url}}
    </div>

    <ul>
      {% for document_ref in refs %}
      <li class="mb-4">
        {{ document_ref | safe }}
      </li>
      {% endfor %}
    </ul>
  </div>
</div>
