{
  "name": "typed-v",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "description": "like v, but typed!",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build1": "NODE_ENV=development rollup -c",
    "build2": "NODE_ENV=development tsc -p src/web-app/tsconfig.json --noEmitOnError --module amd --out dist/out.js",
    "build2-debug": "yarn build2 --diagnostics --listEmittedFiles --listFiles",
    "build2-debug-extened": "yarn build2 --traceResolution",
    "start-server": "NODE_NO_WARNINGS=1 LOG=DEBUG ./vnode src/isodb-server/bin 8080 pass /tmp/db",
    "start-server-with-data": "yarn start-server --gen-data",
    "start-client": "rollup -c --watch",
    "test": "NODE_NO_WARNINGS=1 LOG=ERROR ./vnode src/tester/bin",
    "test-update": "yarn test --update-snapshots",
    "lint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.ts*'",
    "check-compiler-errors": "tsc --noEmit --project tsconfig.json",
    "cloc": "cloc --vcs=git --exclude-ext=json,md,test.js -v *"
  },
  "dependencies": {
    "busboy": "^0.3.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "typestyle": "^2.0.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/node": "^12.0.2",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "rollup": "^1.12.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "^3.2.2"
  }
}
